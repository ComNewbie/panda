<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.playdata.panda.repository.ClassTestRepository">
    
    <!-- select 수업불러오기 회원아이디에 따른 -->
    <select id="findByClassRegUserId" parameterType="Long" resultType="PandaClass">
    	
    	select
    		*
    	from
    		class
    	where
    		class_reg_user_id = #{userId}

    </select>
    
    
    
    
    
    
    
    
    
</mapper>